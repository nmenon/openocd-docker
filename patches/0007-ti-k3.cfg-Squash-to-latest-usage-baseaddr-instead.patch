From a92d7633397367503f346cff629e79dcc639b41a Mon Sep 17 00:00:00 2001
From: Nishanth Menon <nm@ti.com>
Date: Sat, 5 Dec 2020 09:16:03 -0600
Subject: [PATCH 7/8] ti-k3.cfg: Squash to latest usage - baseaddr instead

dont use ctiaddr anymore..

Change-Id: I0a9918c5356839c3d6abc3d16559fe313cae87c9
Signed-off-by: Nishanth Menon <nm@ti.com>
---
 tcl/target/ti-k3.cfg | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/tcl/target/ti-k3.cfg b/tcl/target/ti-k3.cfg
index 08e2f5e737da..b0ec8d91a738 100644
--- a/tcl/target/ti-k3.cfg
+++ b/tcl/target/ti-k3.cfg
@@ -141,7 +141,7 @@ set CM3_ETBBASE		{0x3C025000}
 set CM3_TPIUBASE	{0x20004000}
 
 # M3 is always present
-cti create $_CTINAME.m3 -dap $_CHIPNAME.dap -ap-num 7 -ctibase [lindex $CM3_CTIBASE 0]
+cti create $_CTINAME.m3 -dap $_CHIPNAME.dap -ap-num 7 -baseaddr [lindex $CM3_CTIBASE 0]
 set _command "target create $_TARGETNAME.m3 cortex_m -dap $_CHIPNAME.dap -ap-num 7"
 set _command "$_command -defer-examine"
 eval $_command
@@ -180,7 +180,7 @@ set _v8_smp_targets ""
 for { set _core 0 } { $_core < $_armv8_cores } { incr _core } {
 
 	cti create $_CTINAME.$_armv8_cpu_name.$_core -dap $_CHIPNAME.dap -ap-num 1 \
-		-ctibase [lindex $ARMV8_CTIBASE $_core]
+		-baseaddr [lindex $ARMV8_CTIBASE $_core]
 
 	set _command "target create $_TARGETNAME.$_armv8_cpu_name.$_core aarch64 -dap $_CHIPNAME.dap \
 		-dbgbase [lindex $ARMV8_DBGBASE $_core] -cti $_CTINAME.$_armv8_cpu_name.$_core"
@@ -243,7 +243,7 @@ if { ![info exists R5_ETMBASE] } {
 
 for { set _core 0 } { $_core < $_r5_cores } { incr _core } {
 	cti create $_CTINAME.r5.$_core -dap $_CHIPNAME.dap -ap-num 1 \
-		-ctibase [lindex $R5_CTIBASE $_core]
+		-baseaddr [lindex $R5_CTIBASE $_core]
 	set _command "target create $_TARGETNAME.r5.$_core cortex_r4 -dap $_CHIPNAME.dap \
 		-dbgbase [lindex $R5_DBGBASE $_core] -ap-num 1"
 	# inactive core examination will fail - wait till startup of additional core
@@ -297,7 +297,7 @@ if { ![info exists _mcu_m4_cores] } {
 	set _mcu_m4_cores 0
 }
 if { $_mcu_m4_cores != 0 } {
-	cti create $_CTINAME.m4 -dap $_CHIPNAME.dap -ap-num 8 -ctibase [lindex $CM4_CTIBASE 0]
+	cti create $_CTINAME.m4 -dap $_CHIPNAME.dap -ap-num 8 -baseaddr [lindex $CM4_CTIBASE 0]
 	set _command "target create $_TARGETNAME.m4 cortex_m -dap $_CHIPNAME.dap -ap-num 8"
 	set _command "$_command -defer-examine"
 	eval $_command
-- 
